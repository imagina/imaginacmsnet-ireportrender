<body>

    @foreach (var workOrder in Model)
    {
            DateTime? acStartDate = null;
          DateTime? acEndDate = null;
          float? acTime = null;

            string? acParkingStart = workOrder.workOrderItems.Where(item => item.product.external_id == "SI008").Select(item => item.workOrderItemAttributes.Where(woia => woia.name == "Start").First().value).FirstOrDefault();
            string? acParkingEnd = workOrder.workOrderItems.Where(item => item.product.external_id == "SI008").Select(item => item.workOrderItemAttributes.Where(woia => woia.name == "End").First().value).FirstOrDefault();

        if (!string.IsNullOrEmpty(acParkingStart) && !string.IsNullOrEmpty(acParkingEnd))
        {
            

                    string[] validformats = new[] { "MM/dd/yyyy", "MM/dd/yyyy HH:mm:ss",
                                        "MM/dd/yyyy HH:mm" };

                    CultureInfo provider = new CultureInfo("en-US");

                    acStartDate = DateTime.ParseExact(acParkingStart.Trim(), validformats, provider);
                    acEndDate = DateTime.ParseExact(acParkingEnd.Trim(), validformats, provider);

                         if (acStartDate.HasValue && acEndDate.HasValue)
                    {

                        TimeSpan diff = acEndDate.Value - acStartDate.Value;
                        acTime = (float)Math.Round((float)(diff.TotalHours),2);

                    }
        }

                    DateTime? gpuStartDate = null;
                    DateTime? gpuEndDate = null;
                    float? gpuTime = null;
                    TimeSpan diffGPU = TimeSpan.Zero;
                    string? gpuStart = null;
                    string? gpuEnd = null;
      
                    try
                    {
                        gpuStart = workOrder.workOrderItems.Where(item => item.product.id == 32).Select(item => item.workOrderItemAttributes.Where(woia => woia.name == "Start").First().value).FirstOrDefault();
                        gpuEnd = workOrder.workOrderItems.Where(item => item.product.id == 32).Select(item => item.workOrderItemAttributes.Where(woia => woia.name == "End").First().value).FirstOrDefault();

                           string[] validformats = new[] { "MM/dd/yyyy", "MM/dd/yyyy HH:mm:ss",
                                        "MM/dd/yyyy HH:mm" };

                        CultureInfo provider = new CultureInfo("en-US");

                        gpuStartDate = DateTime.ParseExact(gpuStart.Trim(), validformats, provider);
                        gpuEndDate = DateTime.ParseExact(gpuEnd.Trim(), validformats, provider);

                         if (gpuStartDate.HasValue && gpuEndDate.HasValue)
                    {

                        TimeSpan diff = gpuEndDate.Value - gpuStartDate.Value;
                        gpuTime = (float)Math.Round((float)(diff.TotalHours),2);

                    }
                    
                    }
                    catch (Exception e) { }
