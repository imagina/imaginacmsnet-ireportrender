                        </div>
                    </div>

                </div>


           
                <div class="col-4 mb-2">
                    <div class="card">
                        <div class="card-header">CREW </div>
                        <div class="card-body">

                            <table class="table table-bordered table-sm">
                                <thead>
                                    <tr>
                                        <th>Position</th>
                                        <th>Qty</th>
                                        <th>Hours</th>
                                        <th>OT Hours</th>
                                    </tr>
                                </thead>
                                <tbody>
                                        @{
                                            long supervisorHeadcount = 0;
                                            Int64.TryParse(workOrder.workOrderItems.Where(item => item.product.id == 160).Select(item => item.workOrderItemAttributes.First().value).FirstOrDefault(),out supervisorHeadcount);

                                            long leadHeadcount = 0;
                                            Int64.TryParse(workOrder.workOrderItems.Where(item => item.product.id == 161).Select(item => item.workOrderItemAttributes.First().value).FirstOrDefault(),out leadHeadcount);

                                            long agentHeadcount = 0;
                                            Int64.TryParse(workOrder.workOrderItems.Where(item => item.product.id == 162).Select(item => item.workOrderItemAttributes.First().value).FirstOrDefault(),out agentHeadcount);

                                            long totalHeadcounts = supervisorHeadcount + leadHeadcount + agentHeadcount;


                                               long supervisorHours = 0;
                                            Int64.TryParse(workOrder.workOrderItems.Where(item => item.product.id == 86).Select(item => item.workOrderItemAttributes.First().value).FirstOrDefault(),out supervisorHours);

                                            long leadHours = 0;
                                            Int64.TryParse(workOrder.workOrderItems.Where(item => item.product.id == 79).Select(item => item.workOrderItemAttributes.First().value).FirstOrDefault(),out leadHours);

                                            long agentHours = 0;
                                            Int64.TryParse(workOrder.workOrderItems.Where(item => item.product.id == 163).Select(item => item.workOrderItemAttributes.First().value).FirstOrDefault(),out agentHours);

                                            long totalHours = supervisorHours + leadHours + agentHours;


                                              long supervisorOvertime = 0;
                                            Int64.TryParse(workOrder.workOrderItems.Where(item => item.product.id == 87).Select(item => item.workOrderItemAttributes.First().value).FirstOrDefault(),out supervisorOvertime);

                                            long leadOvertime = 0;
                                            Int64.TryParse(workOrder.workOrderItems.Where(item => item.product.id == 80).Select(item => item.workOrderItemAttributes.First().value).FirstOrDefault(),out leadOvertime);

                                            long agentOvertime = 0;
                                            Int64.TryParse(workOrder.workOrderItems.Where(item => item.product.id == 164).Select(item => item.workOrderItemAttributes.First().value).FirstOrDefault(),out agentOvertime);

                                            long totalOvertime = supervisorOvertime + leadOvertime + agentOvertime;

                               


                                    <tr>
                                        <th>Manager / Supervisor</th>
                                        <td class="text-center">@supervisorHeadcount</td>
                                        <td class="text-center">@supervisorHours</td>
                                        <td class="text-center">@supervisorOvertime</td>
                                    </tr>
                                    <tr>
                                        <th>Agents</th>
                                        <td class="text-center">@agentHeadcount</td>
                                        <td class="text-center">@agentHours</td>
                                        <td class="text-center">@agentOvertime</td>
                                    </tr>
                                    
                                    <tr>
                                        <th>Others</th>
                                        <td class="text-center">@leadHeadcount</td>
                                        <td class="text-center">@leadHours</td>
                                        <td class="text-center">@leadOvertime</td>
                                    </tr>
                                    <tr id="totalCrew">
                                        <th class="table-primary">TOTAL</th>
                                        <td class="table-primary text-center">@totalHeadcounts</td>
                                        <td class="table-primary text-center">@totalHours</td>
                                        <td class="table-primary text-center">@totalOvertime</td>
                                    </tr>
 }
                                </tbody>
                            </table>

                            <table class="table table-bordered table-sm mb-0">
                                <thead>
                                    <tr>
                                        <th>REMARKS</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td class="text-justify p-3" style="height: 154px">
                                            @workOrder.remark
                                        </td>
                                    </tr>
                                </tbody>
                            </table>

                        </div>
                    </div>

                </div>
            }
                <div class="col-6 mb-2 pr-lg-1">
                    <div class="card">
                        <div class="card-header">CF Representative Signature </div>
                        <div class="card-body text-center">
                            <img class="img-fluid" style="max-height: 100px;" src="@(workOrder.representative_signature != null ? workOrder.representative_signature : NullImage)">
                            <table class="table table-bordered table-sm mb-0">
                                <thead>
                                    <tr>
                                        <th>Print Name </th>
                                        <th>Position</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td class="w-40">@workOrder.representative_name</td>
                                        <td>@workOrder.representative_title</td>
                                    </tr>
                                </tbody>
                            </table>

                        </div>
                    </div>
                </div>

                <div class="col-6 mb-2 pl-lg-1">
                    <div class="card">
                        <div class="card-header">Customer Representative Signature</div>
                        <div class="card-body text-center">
                            <img class="img-fluid" style="max-height: 100px;" src="@(workOrder.customer_signature != null ? workOrder.customer_signature : NullImage)">
                            <table class="table table-bordered table-sm mb-0">
                                <thead>
                                    <tr>
                                        <th>Print Name </th>
                                        <th>Position</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td class="w-40">@workOrder.customer_name</td>
                                        <td>@workOrder.customer_title</td>
                                    </tr>
                                </tbody>
                            </table>

                        </div>
                    </div>
                </div>


                <div class="col-12">
                    <!--<p class="text-center mb-0">The Datetimes in this report are using the timezone: <b>America/New_York</b></p>-->
                    <p class="text-right font-weight-bold mb-0">Alliance Ground International @DateTime.Now.Year</p>
                </div>

            </div>

        </section>
       <div style='page-break-before: always;'></div>
    }



</body>
</html>
